<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mybatis.mapper.UuMapper">
    <resultMap id="UserResultMap" type="uutable">
        <id column="id" property="id"/>
        <result column="uu" property="uu"/>
    </resultMap>


    <insert id="insertData"  parameterType="com.mybatis.entity.uutable">
        insert INTO  uutable(id,uu) VALUES (#{id},#{uu})
    </insert>


</mapper>